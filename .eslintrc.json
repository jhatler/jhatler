{
    "root": true,
    "ignorePatterns": [
        "!.eslintrc.json",
        "!.release-please-manifest.json",
        "!.devcontainer"
    ],
    "overrides": [
        {
            "files": [
                "*.json"
            ],
            "extends": [
                "plugin:jsonc/recommended-with-jsonc"
            ],
            "parser": "jsonc-eslint-parser",
            "parserOptions": {
                "jsonSyntax": "JSONC"
            }
        }
    ],
    "rules": {
    }
}
